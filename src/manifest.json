{
    "name": "Video Timer",
    "version": "21.10.27.1",
    "manifest_version": 2,
    "description": "YouTubeを1時間視聴する毎に15分の休憩時間を設けるChrome拡張。",
    "content_scripts": [{
        "matches": ["https://*.youtube.com/*"],
        "css": [
            "default.css"
        ],
        "js": [
            "content.js"
        ]
    }]
}